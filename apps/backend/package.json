{
  "name": "facturation-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config test/jest-e2e.json",
    "db:migrate": "ts-node --swc src/scripts/migrate.ts",
    "db:seed": "ts-node --swc src/scripts/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "10.3.2",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.2",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.3.2",
    "@nestjs/typeorm": "10.0.0",
    "argon2": "0.31.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "2.30.0",
    "nestjs-pino": "4.0.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pg": "8.11.3",
    "pino": "8.15.1",
    "pino-pretty": "10.3.0",
    "puppeteer": "22.7.1",
    "rxjs": "7.8.1",
    "typeorm": "0.3.17",
    "uuid": "9.0.1",
    "joi": "17.12.1",
    "json2csv": "6.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.3.2",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.19",
    "@types/passport-jwt": "3.0.12",
    "@types/supertest": "2.0.16",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}